\begingroup \toks 0={macro:-> \begin {axis} [ xmin=0, xmax=5, ymin=0, ymax=10, axis x line=middle, axis y line=middle, xlabel=\(x\), ylabel=\(y\), grid=both, tick align=outside, minor tick num=1, axis line style={-{Stealth[length=3mm,width=1.5mm]}}, title={Funções \(y=4x-x^2\) e \(y=x^2\)}, ] \addplot [red, domain=0:3, samples=200, thick, name path=A] {4*x-x^2}; \addplot [azulmarinho, domain=0:3, samples=200, thick, name path=B] {x^2}; \node at (axis cs:3.5,3.5) {(3,3)}; \node at (axis cs:2.5,9) {(3,9)}; \draw [red, dashed] (3,3) -- (3,9); \addplot [fill=gray!20, draw=none] fill between[of=A and B, soft clip={domain=0:2}]; \par \addplot [fill=gray!20, draw=none] fill between[of=B and A, soft clip={domain=2:3}]; \end {axis} }\xdef \tikzexternallastkey {\the \toks 0 }\endgroup %
